<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Intiparam</display-name>
 <servlet>
 	<servlet-name>phonedetails</servlet-name>
 	<servlet-class>Mydetails.Mydetails</servlet-class>
 	<init-param>
 		<param-name>name</param-name>
 		<param-value>Windows</param-value>
 	</init-param>
 	<init-param>
 		<param-name>Os</param-name>
 		<param-value>linux</param-value>
 	</init-param>
 </servlet>
 
 
 <servlet-mapping>
 	<servlet-name>phonedetails</servlet-name>
 	<url-pattern>/details</url-pattern>
 </servlet-mapping>
 
 <context-param>
 	<param-name>name</param-name>
 	<param-value>hp</param-value>
 </context-param>
 
 
  <context-param>
 	<param-name>Os</param-name>
 	<param-value>intel</param-value>
 </context-param>
 
</web-app>