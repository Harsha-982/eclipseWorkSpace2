  </servlet>
  
  <servlet-mapping>
  <servlet-name>square</servlet-name>
  <url-pattern>/square</url-pattern>
  </servlet-mapping>


<servlet>
  <servlet-name>2</servlet-name>
  <servlet-class>Demo.MultiplyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>2</servlet-name>
  <url-pattern>/get2table</url-pattern>
  </servlet-mapping>

</web-app>